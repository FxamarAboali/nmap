prefix = @prefix@
datarootdir = @datarootdir@
exec_prefix = @exec_prefix@
bindir = @bindir@
sbindir = @sbindir@
mandir = @mandir@
srcdir = @srcdir@

CC = @CC@
AR = ar
RANLIB = @RANLIB@
CCOPT = 
DEFS = @DEFS@
# With GCC, add extra security checks to source code.
DEFS += -D_FORTIFY_SOURCE=2
CPPFLAGS = @CPPFLAGS@
CFLAGS = @CFLAGS@ $(CCOPT) $(GLIB_CFLAGS) $(DEFS) $(INCLS)
STATIC = 
LDFLAGS = @LDFLAGS@ $(STATIC)
LIBS =  @LIBS@ 
SHTOOL = ${srcdir}/../shtool
INSTALL = $(SHTOOL) install 
MAKEDEPEND = @MAKEDEPEND@

TARGET = libnbase.a

DEPS = ${srcdir}/getopt.h ${srcdir}/nbase.h ${srcdir}/nbase_winconfig.h ${srcdir}/nbase_config.h ${srcdir}/nbase_ipv6.h ${srcdir}/nbase_winunix.h ${srcdir}/nbase_crc32ct.h ${srcdir}/nbase_addrset.h
OBJS = @LIBOBJS@

all: $(TARGET) 

$(TARGET): $(DEPS) $(OBJS)
	rm -f $@
	$(AR) cr $@ $(OBJS)
	$(RANLIB) $@

clean:
	rm -f $(OBJS) $(TARGET) 

distclean: clean
	rm -f Makefile config.cache config.log config.status ${srcdir}/nbase_config.h

depend:
	$(MAKEDEPEND) $(INCLS) -s "# DO NOT DELETE" -- $(DEFS) -- $(SRCS)

${srcdir}/configure: ${srcdir}/configure.ac
	cd $(srcdir) && autoconf

Makefile: ${srcdir}/Makefile.in config.status
	./config.status

config.status: ${srcdir}/configure
	./config.status --recheck

%.o: ${srcdir}/%.c $(DEPS)
	$(CC) -c $(CFLAGS) -c $< -o $@

